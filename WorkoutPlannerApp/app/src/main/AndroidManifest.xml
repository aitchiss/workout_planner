<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codeclan.workoutplannerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ViewWorkoutActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".CreateWorkoutActivity" />
        <activity android:name=".AddSetActivity" />
        <activity android:name=".EditWorkoutActivity" />
        <activity android:name=".PlayWorkoutActivity" />
        <activity android:name=".WorkoutHistoryActivity" />
        <activity android:name=".AddCustomSetActivity" />
        <activity android:name=".AddSuperSetActivity" />
        <activity android:name=".SelectSuperSetActivity"></activity>
    </application>

</manifest>